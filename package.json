{
  "name": "chessboardjs-react",
  "version": "0.0.1",
  "description": "A reactified JavaScript chessboard",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "ci": "npm run lint:js && npm run cover",
    "lint:js": "eslint src/**/*.js test/**/*.js",
    "cover": "nyc mocha --recursive",
    "cover:report": "nyc report --reporter=text-lcov",
    "test": "mocha --require babel-register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mishfit/chessboardjs-react.git"
  },
  "keywords": [
    "chess",
    "chessboard",
    "react"
  ],
  "author": "Mish Ochu <mishochu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mishfit/chessboardjs-react/issues"
  },
  "homepage": "https://github.com/mishfit/chessboardjs-react#readme",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-node": "^6.0.1",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1"
  },
  "engines": {
    "node": ">= 4.x <= 9.x",
    "npm": ">= 2.x <= 5.x"
  }
}
